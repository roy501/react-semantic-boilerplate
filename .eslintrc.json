{
  "env": {
    "node": true,
    "es6": true,
    "commonjs": true
  },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "globals": {
    "jQuery": true,
    "$": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": 0,
    "dot-notation": 2,
    "eqeqeq": [
      2,
      "smart"
    ],
    "array-bracket-spacing": [
      2,
      "never"
    ]
  }
}